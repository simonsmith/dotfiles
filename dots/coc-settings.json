{
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "coc.preferences.colorSupport": false,
  "coc.preferences.formatOnType": true,
  "css.lint.unknownAtRules": "ignore",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "x",
  "diagnostic.infoSign": "~",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false,
    "source.fixAll.stylelint": false
  },
  "eslint.enable": true,
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "eslint.onIgnoredFiles": "off",
  "eslint.options": {
    "cache": true,
    "cacheLocation": ".eslintcache"
  },
  "eslint.run": "onSave",
  "eslint.workingDirectories": ["."],
  "javascript.preferences.noSemanticsValidation": false,
  "javascript.preferences.noSuggestionDiagnostics": true,
  "javascript.referencesCodeLens.enable": false,
  "javascript.suggestionActions.enabled": false,
  "stylelintplus.autoFixOnSave": false,
  "stylelintplus.enable": true,
  "stylelintplus.filetypes": ["css", "scss", "sass", "less", "postcss"],
  "suggest.maxCompleteItemCount": 30,
  "suggest.minTriggerInputLength": 1,
  "suggest.noselect": false,
  "suggest.pumheight": 30,
  "suggest.timeout": 3000,
  "tsserver.maxTsServerMemory": 4096,
  "tsserver.useLocalTsdk": true,
  "tsserver.useSeparateSyntaxServer": false,
  "tsserver.watchOptions": {
    "excludeDirectories": [
      "**/node_modules",
      "**/.git",
      "**/dist",
      "**/build",
      "**/cypress",
      "**/.next",
      "**/coverage",
      "**/.nyc_output",
      "**/tmp",
      "**/temp"
    ]
  },
  "typescript.preferences.lazyConfiguredProjectsFromExternalProject": true,
  "typescript.preferences.noSemanticsValidation": false,
  "typescript.preferences.noSuggestionDiagnostics": true,
  "typescript.referencesCodeLens.enable": false,
  "typescript.suggestionActions.enabled": false,
  "typescript.surveys.enabled": false,
  "vue.codeLens.enabled": false
}
